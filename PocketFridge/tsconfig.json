{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,

    "jsx": "react-jsx",

    "module": "esnext",
    "moduleResolution": "node",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },

    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}